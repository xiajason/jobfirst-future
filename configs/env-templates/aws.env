# AWS环境配置模板
# 部署到AWS时使用

# 服务器信息
CLOUD_PROVIDER=aws
SERVER_IP=aws-instance-ip

# 数据库配置（AWS RDS）
DB_HOST=jobfirst.xxxxx.region.rds.amazonaws.com
DB_PORT=3306
DB_USER=admin
DB_PASSWORD=AWS_SecurePassword_2025
DB_NAME=jobfirst_aws

# Redis配置（AWS ElastiCache）
REDIS_HOST=jobfirst-redis.xxxxx.region.cache.amazonaws.com
REDIS_PORT=6379
REDIS_PASSWORD=AWS_Redis_Password_2025

# PostgreSQL配置（AWS RDS）
POSTGRES_HOST=jobfirst-pg.xxxxx.region.rds.amazonaws.com
POSTGRES_PORT=5432
POSTGRES_USER=postgres
POSTGRES_PASSWORD=AWS_PG_Password_2025
POSTGRES_DB=jobfirst_aws

# MongoDB配置（AWS DocumentDB）
MONGODB_HOST=jobfirst-mongo.xxxxx.region.docdb.amazonaws.com
MONGODB_PORT=27017
MONGODB_USER=admin
MONGODB_PASSWORD=AWS_Mongo_Password_2025
MONGODB_DB=jobfirst_aws

# JWT配置（跨云统一）
JWT_SECRET=jobfirst-unified-auth-secret-key-2024

# MinerU服务地址
MINERU_SERVICE_URL=http://aws-mineru-service:8621

# 天翼云认证中心（跨云调用）
AUTH_CENTER_URL=http://101.33.251.158:8207

# 服务端口配置（与阿里云保持一致）
USER_SERVICE_PORT=8081
RESUME_SERVICE_PORT=8082
JOB_SERVICE_PORT=8083
BANNER_SERVICE_PORT=8084
TEMPLATE_SERVICE_PORT=8085
STATISTICS_SERVICE_PORT=8086
NOTIFICATION_SERVICE_PORT=8087
COMPANY_SERVICE_PORT=8088
DEV_TEAM_SERVICE_PORT=8089
API_GATEWAY_PORT=8601

# AWS特定配置
AWS_REGION=us-west-2
AWS_S3_BUCKET=jobfirst-files
AWS_CLOUDWATCH_LOG_GROUP=/aws/jobfirst

